
// ========================================
// Stylus mixins
// ========================================

// Support for ie defaulting to true.
support-for-ie = true

// Default vendor prefixes.
vendor-prefixes ?= webkit moz o ms official

// Include mixins
@import 'vendor'
@import 'border'
@import 'clearfix'
@import 'flex'
@import 'font-family'
@import 'gradients'
@import 'image-replace'
@import 'image'
@import 'positions'
@import 'text'
